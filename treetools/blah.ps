/highlightbox
{
  gsave
  3.000000 setlinewidth
  0.0 -18.000000 rmoveto 0 60.000000 rlineto dup 0.0 rlineto 0 -60.000000 rlineto neg 0 rlineto
  gsave
  setrgbcolor fill
  grestore
  stroke
  grestore
} bind def

/specshow
{
  save
  gsave
  leaffont setfont
  exch
  /specname exch def
  exch
  /leafname exch def
  exch
  1 eq
  {
    leafname stringwidth pop
    specname stringwidth pop
    dup 0.0 ne
    {
      (\(\) ) stringwidth pop add
    }
    if
    add neg
    0.0 rmoveto
  }
  if
  exch
  dup
  0.0 ge
  {
    /b exch def
    exch /g exch def
    exch /r exch def
    r g b
    leafname stringwidth pop
    specname stringwidth pop
    dup 0.0 ne
    {
      (\(\) ) stringwidth pop add
    }
    if
    add
    highlightbox
  }
  {
    pop exch pop exch pop
  }
  ifelse
  exch
  dup
  0.0 ge
  {
    /b exch def
    exch /g exch def
    exch /r exch def
    r g b setrgbcolor
  }
  {
    pop exch pop exch pop
  }
  ifelse
  leaffont setfont leafname show
  specname stringwidth pop 0.0 ne
  {
    specfont setfont ( \() show specname show (\)) show
  }
  if
  grestore
  restore
} bind def
